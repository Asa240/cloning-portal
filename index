<!-- Toast Notification -->
<div class="toast-notification" id="toast-notification">
<div class="toast-icon">
<span class="material-icons">check_circle</span>
</div>
<div class="toast-message">
<div class="toast-title">New Account Created</div>
<div class="toast-content" id="toast-content">z3n-my_demo_account.zendesk.com</div>
</div>
<button class="toast-close" id="toast-close">
<span class="material-icons">close</span>
</button>
</div>
<script>
    document.addEventListener('DOMContentLoaded', function() {
      // Page navigation
      const templatesTab = document.getElementById('templates-tab');
      const accountsTab = document.getElementById('accounts-tab');
      const templatesPage = document.getElementById('templates-page');
      const accountsPage = document.getElementById('cloned-accounts-page');
      
      templatesTab.addEventListener('click', function() {
        templatesTab.classList.add('active');
        accountsTab.classList.remove('active');
        templatesPage.style.display = 'block';
        accountsPage.style.display = 'none';
      });
      
      accountsTab.addEventListener('click', function() {
        accountsTab.classList.add('active');
        templatesTab.classList.remove('active');
        accountsPage.style.display = 'block';
        templatesPage.style.display = 'none';
      });
      
      // Category filtering
      const filterTabs = document.querySelectorAll('.filter-tab');
      const templateCards = document.querySelectorAll('.template-card');
      const emptyState = document.getElementById('empty-state');
      const searchInput = document.getElementById('search-input');
      
      let activeFilter = 'all';
      let searchQuery = '';
      
      function filterTemplates() {
        let visibleCount = 0;
        
        templateCards.forEach(card => {
          const category = card.getAttribute('data-category');
          const title = card.querySelector('.template-title').textContent.toLowerCase();
          const subtitle = card.querySelector('.template-subtitle').textContent.toLowerCase();
          
          const matchesCategory = activeFilter === 'all' || category === activeFilter;
          const matchesSearch = searchQuery === '' || 
                               title.includes(searchQuery) || 
                               subtitle.includes(searchQuery);
          
          if (matchesCategory && matchesSearch) {
            card.style.display = 'flex';
            visibleCount++;
          } else {
            card.style.display = 'none';
          }
        });
        
        // Show/hide empty state
        if (visibleCount === 0) {
          emptyState.style.display = 'block';
        } else {
          emptyState.style.display = 'none';
        }
      }
      
      filterTabs.forEach(tab => {
        tab.addEventListener('click', function() {
          filterTabs.forEach(t => t.classList.remove('active'));
          tab.classList.add('active');
          
          activeFilter = tab.getAttribute('data-category');
          filterTemplates();
        });
      });
      
      searchInput.addEventListener('input', function() {
        searchQuery = this.value.toLowerCase();
        filterTemplates();
      });
      
      // Modal functionality
      const templateModal = document.getElementById('template-modal');
      const closeModal = document.getElementById('close-modal');
      const modalTitle = document.getElementById('modal-title');
      const modalDescription = document.getElementById('modal-description');
      
      // Open modal when clicking on template card
      templateCards.forEach(card => {
        card.addEventListener('click', function() {
          const title = card.querySelector('.template-title').textContent;
          const category = card.getAttribute('data-category');
          
          modalTitle.textContent = title;
          
          // Set description based on template category
          let description = '';
          switch(category) {
            case 'retail':
              description = 'A complete retail demonstration environment with pre-configured products, customer profiles, and order management workflows tailored to showcase retail-specific features.';
              break;
            case 'tech':
              description = 'A technology company demonstration environment with subscription management, user roles, and analytics dashboards tailored to showcase SaaS-specific features.';
              break;
            case 'travel':
              description = 'A travel and hospitality demonstration environment with booking management, itineraries, and customer profiles tailored to showcase travel-specific features.';
              break;
            case 'finance':
              description = 'A financial services demonstration environment with account management, transaction history, and security features tailored to showcase finance-specific workflows.';
              break;
            case 'healthcare':
              description = 'A healthcare demonstration environment with patient records, appointment scheduling, and compliance features tailored to showcase healthcare-specific workflows.';
              break;
            default:
              description = 'This template provides a pre-configured environment for demonstrating industry-specific workflows and features.';
          }
          
          modalDescription.textContent = description;
          
          // Reset checkboxes
          const checkboxes = templateModal.querySelectorAll('input[type="checkbox"]');
          checkboxes.forEach(checkbox => {
            checkbox.checked = false;
          });
          
          // Show date range if import data is checked
          document.getElementById('date-range-container').style.display = 'none';
          
          templateModal.style.display = 'flex';
        });
      });
      
      // Import data checkbox handler
      const importDataCheckbox = document.getElementById('import-data-checkbox');
      const dateRangeContainer = document.getElementById('date-range-container');
      
      importDataCheckbox.addEventListener('change', function() {
        if (this.checked) {
          dateRangeContainer.style.display = 'block';
        } else {
          dateRangeContainer.style.display = 'none';
        }
      });
      
      // Close modal
      closeModal.addEventListener('click', function() {
        templateModal.style.display = 'none';
      });
      
      document.getElementById('cancel-button').addEventListener('click', function() {
        templateModal.style.display = 'none';
      });
      
      // Close modal when clicking outside
      window.addEventListener('click', function(event) {
        if (event.target === templateModal) {
          templateModal.style.display = 'none';
        }
      });
      
      // Create instance button
      const createInstanceButton = document.getElementById('create-instance-button');
      const toastNotification = document.getElementById('toast-notification');
      const toastContent = document.getElementById('toast-content');
      const toastClose = document.getElementById('toast-close');
      
      createInstanceButton.addEventListener('click', function() {
        // Generate a random account name based on the selected template
        const templateName = modalTitle.textContent.toLowerCase().replace(/[^a-z0-9]/g, '_');
        const randomId = Math.floor(Math.random() * 1000);
        const accountUrl = `z3n-${templateName}_${randomId}.zendesk.com`;
        
        // Update toast content
        toastContent.textContent = accountUrl;
        
        // Hide modal
        templateModal.style.display = 'none';
        
        // Show toast notification
        toastNotification.classList.add('show');
        
        // Hide toast after 5 seconds
        setTimeout(function() {
          toastNotification.classList.remove('show');
        }, 5000);
      });
      
      // Close toast
      toastClose.addEventListener('click', function() {
        toastNotification.classList.remove('show');
      });
    });
  </script>
</body>
</div></div>
<script>
  document.addEventListener('DOMContentLoaded', function() {
    const toggleButton = document.getElementById('toggle-demo-services');
    const content = document.getElementById('demo-services-content');
    if (toggleButton && content) {
      toggleButton.addEventListener('click', () => {
        content.style.display = content.style.display === 'none' ? 'block' : 'none';
      });
    }
  });
</script>
</body></html>
